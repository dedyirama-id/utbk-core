{
  "name": "utbk-core",
  "private": true,
  "devDependencies": {
    "cross-env": "^7.0.3",
    "git-format-staged": "^3.1.1",
    "husky": "^9.0.11",
    "lerna": "^8.1.3",
    "prettier": "3.3.2"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=production lerna run start --scope backend",
    "build": "lerna run build",
    "test": "lerna run test",
    "lint": "lerna run lint",
    "clean": "lerna run clean",
    "bootstrap": "lerna bootstrap",
    "dev": "cross-env NODE_ENV=development lerna run dev --parallel",
    "dev:backend": "lerna run dev --scope backend",
    "dev:frontend": "lerna run dev --scope frontend"
  }
}
